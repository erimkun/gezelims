{
  "name": "data-restorer",
  "version": "1.0.0",
  "description": "GeoJSON data categorization and cleaning using AI models",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "node src/test.js",
    "gemini": "node src/index.js --provider google --input ../pearl_of_the_istanbul/public/data",
    "openai": "node src/index.js --provider openai --input ../pearl_of_the_istanbul/public/data",
    "claude": "node src/index.js --provider anthropic --input ../pearl_of_the_istanbul/public/data",
    "ollama": "node src/index.js --provider ollama --input ../pearl_of_the_istanbul/public/data",
    "openrouter": "node src/index.js --provider openrouter --input ../pearl_of_the_istanbul/public/data",
    "copilot": "node src/index.js --provider vscode-copilot --input ../pearl_of_the_istanbul/public/data"
  },
  "keywords": [
    "geojson",
    "ai",
    "categorization",
    "ollama",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "ollama": "^0.5.0",
    "openai": "^4.20.0",
    "@anthropic-ai/sdk": "^0.9.0",
    "@google/generative-ai": "^0.1.0"
  }
}